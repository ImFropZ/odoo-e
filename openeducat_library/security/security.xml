<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- Teacher Group For Library -->
  <record id="group_library_teacher" model="res.groups">
    <field name="name">Teacher</field>
    <field name="category_id" ref="openeducat_core.module_category_all_other_education" />
    <field name="implied_ids"
      eval="[(4, ref('openeducat_core.group_school_teacher'))]" />
  </record>

  <!-- Group Teacher -->
  <record id="openeducat_core.group_school_teacher" model="res.groups">
    <field name="implied_ids"
      eval="[(4, ref('openeducat_library.group_library_teacher'))]" />
  </record>

</odoo>