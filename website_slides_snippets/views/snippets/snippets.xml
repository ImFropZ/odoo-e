<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="custom_website_slide_snippets" name="Website Slide Snippets" inherit_id="website.snippets" priority="8">
        <xpath expr="//div[@id='snippet_effect']//t[@t-snippet][last()]" position="after">
            <t t-snippet="website_slides_snippets.dynamic_slide_snippet" />
        </xpath>
    </template>
</odoo>